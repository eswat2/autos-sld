{
  "name": "autos-sld",
  "version": "0.0.86",
  "description": "",
  "scripts": {
    "dev": "concurrently \"yarn:twx --watch\" \"yarn:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "build": "yarn twx; yarn format; vite build",
    "serve": "vite preview",
    "spell": "cspell *.{html,json,js,md,ts} src/*.{css,tsx}",
    "cc": "prettier --check src",
    "format": "prettier --write src",
    "predeploy": "yarn version --patch; yarn build",
    "deploy": "cd dist; ln -s ../.vercel; vc --prod",
    "open": "open https://autos-sld.vercel.app"
  },
  "dependencies": {
    "solid-js": "1.3.3"
  },
  "devDependencies": {
    "autoprefixer": "10.4.2",
    "concurrently": "7.0.0",
    "fetchql": "3.0.0",
    "postcss": "8.4.5",
    "proto-tailwindcss-clrs": "0.0.89",
    "tailwindcss": "3.0.13",
    "vite": "2.7.11",
    "vite-plugin-solid": "2.2.2"
  },
  "license": "MIT"
}
