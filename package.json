{
  "name": "autos-sld",
  "version": "0.0.1093",
  "type": "module",
  "description": "",
  "scripts": {
    "dev": "concurrently \"yarn:twx --watch\" \"yarn:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "build": "yarn twx; yarn format; vite build",
    "serve": "vite preview",
    "spell": "cspell *.{html,json,js,md,ts} src/*.{css,tsx}",
    "cc": "prettier --check src",
    "format": "prettier --write src",
    "predeploy": "yarn version --patch; yarn build",
    "deploy": "cd dist; ln -s ../.vercel; vc --prod",
    "open": "open https://autos-sld.vercel.app"
  },
  "dependencies": {
    "solid-js": "1.9.4"
  },
  "devDependencies": {
    "autoprefixer": "10.4.20",
    "concurrently": "9.1.2",
    "fetchql": "3.0.0",
    "postcss": "8.5.3",
    "prettier": "3.5.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "proto-tailwindcss-clrs": "0.0.410",
    "tailwindcss": "4.0.8",
    "vite": "6.1.1",
    "vite-plugin-solid": "2.11.2"
  },
  "license": "MIT"
}
