{
  "name": "autos-sld",
  "version": "0.0.1337",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "bump": "bumpp patch --commit --tag --push --yes",
    "deploy-app": "cd dist; ln -s ../.vercel; vc --prod",
    "release": "pnpm bump && pnpm build && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"pnpm:twx --watch\" \"pnpm:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "build": "pnpm twx; pnpm format; vite build",
    "serve": "vite preview",
    "spell": "cspell *.{html,json,js,md,ts} src/*.{css,tsx}",
    "cc": "prettier --check src",
    "format": "prettier --write src",
    "open": "open https://autos-sld.vercel.app"
  },
  "dependencies": {
    "solid-js": "1.9.11"
  },
  "devDependencies": {
    "autoprefixer": "10.4.24",
    "bumpp": "10.4.0",
    "concurrently": "9.2.1",
    "fetchql": "3.0.0",
    "only-allow": "1.2.2",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "proto-tailwindcss-clrs": "0.0.464",
    "tailwindcss": "4.1.18",
    "vite": "7.3.1",
    "vite-plugin-solid": "2.11.10"
  },
  "license": "MIT"
}
